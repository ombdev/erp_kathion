    <name>fuente_hechos</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>erp_db_conn</connection>
    <sql>select  alfa.numero_control,
       alfa.ciclo_creacion, alfa.mes_creacion, alfa.dia_creacion,
       alfa.geo_id, (select sku from inv_prod where inv_prod.id = pd.producto_id),
       TRUNC((pd.cantidad * (pd.precio_unitario * 1.16))::numeric,2) as importe, pd.cantidad
from erp_prefacturas_detalles as pd
JOIN (
    SELECT pre.id, set_facturados.proceso_id, cxc_clie.numero_control,
          cxc_clie.razon_social,
          EXTRACT(YEAR FROM set_facturados.momento_creacion) AS ciclo_creacion,
          EXTRACT(MONTH FROM set_facturados.momento_creacion) AS mes_creacion,
          EXTRACT(DAY FROM set_facturados.momento_creacion) AS dia_creacion,     
          (SELECT gral_suc.gral_mun_id as geo_id
           FROM gral_usr_suc JOIN gral_suc ON gral_suc.id=gral_usr_suc.gral_suc_id
           WHERE gral_usr_suc.gral_usr_id = set_facturados.gral_usr_id_creacion )
    from erp_prefacturas as pre
    JOIN ( select f.proceso_id, f.momento_creacion, f.gral_usr_id_creacion
          from fac_docs as f
          join erp_proceso as p ON p.id = f.proceso_id
          where NOT f.cancelado AND p.empresa_id = 7 AND f.moneda_id = 1 AND f.momento_creacion >= timestamp '2017-01-01' ) as set_facturados
    ON set_facturados.proceso_id = pre.proceso_id
    JOIN cxc_clie
    ON cxc_clie.id = pre.cliente_id
) as alfa
ON alfa.id = pd.prefacturas_id ORDER by pd.prefacturas_id</sql>
